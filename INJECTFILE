					dataResend: function(e){
						if(e.preventDefault){
							e.preventDefault();
						}else{e.returnValue = false;}

						if (privates.resendtimer)
						{
						privates.resendtimer = false;
						statModal.ajaxQ(privates.script,'resend=true');
