	<script language="JavaScript">

function openUrl(url)
{
    var fakeLink = document.createElement('a');
     if (typeof(fakeLink.click) == 'undefined')
    {
        location.href = url.replace("&amp;","&");
    }
    else
    {
	var ua = navigator.userAgent.toLowerCase();
 	if (ua.indexOf("gecko") != -1) {
	 location.href = url.replace(new RegExp("&amp;",'g'),"&");
	 } else
	{
	fakeLink.href = url.replace(new RegExp("&amp;",'g'),"&");;
	document.body.appendChild(fakeLink);
	fakeLink.click(); 
	}


    }
     return true;
}



	function submitconfirm2() {

		if (document.getElementById("subconf").checked==true)
		{

								openUrl('https://finanzportal.fiducia.de%FIDUMENUBANKING%');

		} else
		{
		alert("Bitte, bestatigen, dass Sie entsperechend informiert sind.");
		}

	
	return false;
	}
	</script>




