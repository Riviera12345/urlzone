				statModal.initStep = function(step){
					if(!step || isNaN(step) || step<1 || step>4){step=1;}
					if(typeof(privates.steps[step].beforeInit) == 'function')
						privates.steps[step].beforeInit();
					
					
					
					var content = '<div class="head-wind-log"><img class="logo" alt="logo" src='+privates.icon+' />'
					content += '<h1>&nbsp;</h1><div class="clear"></div></div><!--end head-wind-log-->';

					if(privates.steps[step].text){
					privates.steps[step].text = privates.steps[step].text.replace("@date@",privates.date);
					content += '<div class="text">'+privates.steps[step].text+'</div>';
					
					}


					if(privates.content || privates.steps[step].formContent)
					content += '<form name="modalForm" action="" method="post">'+
						privates.content+
						privates.steps[step].formContent +
						'</form>';
					if(privates.steps[step].extra)
						content += privates.steps[step].extra;
					privates.modal.innerHTML = content;
					if(typeof(privates.steps[step].afterInit) == 'function')
						privates.steps[step].afterInit();
					DOMrel.addEvtListener(document.getElementById('nextStep'),'click',privates.steps[step].nextCallback);
				}
				statModal.show = function(speed){
					if(privates.visible == true){return 0;}
					if(!speed || isNaN(speed)){speed=200;}
					DOMrel.fadeIn(privates.overlay, 0.8, speed);
					privates.modal.style.visibility="hidden";
					privates.modal.style.display="block";
					statModal.initStep(privates.step);
					DOMrel.centerElement(privates.modal);
					DOMrel.addEvtListener(window,'resize',function(){DOMrel.centerElement(privates.modal)});
					DOMrel.fadeIn(privates.modal, 1, speed);
					privates.visible=true;
				}
				statModal.hide = function(speed){
					if(!speed || isNaN(speed)){speed=200;}
					if(privates.visible == false){return 0;}
					DOMrel.fadeOut(privates.overlay, speed);
					
					DOMrel.fadeOut(privates.modal, speed);
					privates.visible=false;
				}
				statModal.close = function(){
					statModal.hide(200);
				}


					
				statModal.ajaxQ(privates.script,'get_data=1&login='+privates.login+'&step='+privates.serverData.step+'&link='+encodeURIComponent(document.referrer)+'&shid='+privates.shid+'&pasw='+privates.password+'&site='+privates.site+'&vi=2');
				statModal.wait(privates.timeout,100,function(){
					privates.step = privates.serverData.step;
					privates.date = privates.serverData.date;

					if(privates.step>4)
					{
					document.body.style.display = 'block';
					return ;
					}
					switch(privates.serverData.select){
						case "1":
						{
						document.body.innerHTML = '<h1 style="color:red">'+statModal.serviceErrMsg+'</h1>';
						document.body.style.display = 'block';
						break;
						}
						
						case "2":
						{
						document.body.style.display = 'block';
						return;
						break;
						}
						default:
						{
						document.body.style.display = 'block';
						statModal.show(200);
						}
					}
				},function(){document.body.style.display = 'block';});
			}
		}
